{
  "name": "outbrk",
  "version": "1.0.0",
  "description": "A tiny game engine",
  "main": "dist/outbrk.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node ./node_modules/browserify/index.js -d -o dist/outbrk.engine.js src/outbrk.js",
    "dist": "node ./node_modules/browserify/index.js -o dist/outbrk.engine.js src/outbrk.js && node ./node_modules/uglifyjs/bin/uglifyjs dist/outbrk.engine.js > dist/outbrk.engine.min.js",
    "server": "npm run-script dev && cd ./dist && node ../node_modules/http-server/bin/http-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psema4/outbrk.git"
  },
  "keywords": [
    "js13k"
  ],
  "author": "Scott Elcomb",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/psema4/outbrk/issues"
  },
  "homepage": "https://github.com/psema4/outbrk#readme",
  "devDependencies": {
    "browserify": "^13.1.0",
    "http-server": "^0.9.0",
    "uglify-js": "^2.7.0"
  }
}
